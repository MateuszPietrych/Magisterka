&lt;infobox&gt;
 &lt;title source="name"&gt;&lt;default&gt;Feainnewedd&lt;/default&gt;&lt;/title&gt;
 &lt;image source="image"&gt;
 &lt;caption source="caption" /&gt;&lt;/image&gt;
